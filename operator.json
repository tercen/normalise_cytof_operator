{
  "name": "Normalise CyTOF",
  "description": "Normalisation of CyTOF data using beads.",
  "tags": [ "data preprocessing", "cytof"],
  "container":"tercen/normalise_cytof_operator:latest",
  "authors": ["tercen"],
  "urls": ["https://github.com/tercen/normalise_cytof_operator"],
  "properties": [
    {
    "kind": "EnumeratedProperty",
    "name": "beads",
    "defaultValue": "dvs",
    "description": "Beads to be used for normalisation: dvs (for bead masses 140, 151, 153 ,165, 175) or beta (for bead masses 139, 141, 159, 169, 175).",
  "values": [
      "dvs",
      "beta"
      ]
    },
    {
    "kind": "DoubleProperty",
    "name": "plot_width",
    "defaultValue": 500,
    "description": "Diagnostic plot width (pixels)."
    },
    {
    "kind": "DoubleProperty",
    "name": "plot_height",
    "defaultValue": 500,
    "description": "Diagnostic plot height (pixels)."
    }
  ]
}


